NODE_ENV=development
PORT=3333
LOG_LEVEL=warn
LOG_REQUESTS=false

DATABASE_URL=mysql://user:password@host:3306/database

JWT_ACCESS_SECRET=troque_esta_chave_access
JWT_REFRESH_SECRET=troque_esta_chave_refresh
ACCESS_TOKEN_EXPIRES_IN=1h
REFRESH_TOKEN_EXPIRES_IN_DAYS=30

APP_ENCRYPTION_KEY=troque_esta_chave_de_criptografia

EVOLUTION_BASE_URL=http://localhost:8080
EVOLUTION_API_KEY=
EVOLUTION_INSTANCE_NAME=agente_nfe
SEFAZ_TP_AMB=1
# Codigo IBGE da UF (ex.: 29=BA, 35=SP, 41=PR)
SEFAZ_CUF_AUTOR=35
SEFAZ_NFE_DIST_PROD_URL=https://www1.nfe.fazenda.gov.br/NFeDistribuicaoDFe/NFeDistribuicaoDFe.asmx
SEFAZ_NFE_DIST_HOMOLOG_URL=https://hom1.nfe.fazenda.gov.br/NFeDistribuicaoDFe/NFeDistribuicaoDFe.asmx
SEFAZ_TIMEOUT_MS=30000
SEFAZ_MAX_BATCHES_PER_SYNC=5
SYNC_MIN_INTERVAL_SECONDS=3660
AGENT_WHATSAPP_NUMBER=

GROQ_API_KEY=
GROQ_MODEL=llama-3.1-8b-instant

# true para rodar o sync horario junto com a API.
ENABLE_EMBEDDED_WORKER=false

# true para servir apps/web/dist na mesma porta da API.
SERVE_WEB_STATIC=false
